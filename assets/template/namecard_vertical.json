[
  {
    "type": "flex",
    "altText": "this is a flex message",
    "contents": {
      "type": "bubble",
      "size": "giga",
      "body": {
        "type": "box",
        "layout": "vertical",
        "paddingAll": "0px",
        "contents": [
          {
            "type": "image",
            "url": "${data.backgroundUrl}",
            "aspectMode": "cover",
            "gravity": "center",
            "size": "full",
            "aspectRatio": "2:3"
          },
          {
            "layout": "vertical",
            "paddingAll": "40px",
            "width": "100%",
            "height": "100%",
            "alignItems": "center",
            "type": "box",
            "position": "absolute",
            "backgroundColor": "${data.backgroundColor}",
            "contents": [
              {
                "layout": "vertical",
                "type": "box",
                "justifyContent": "center",
                "flex": "${data.topFlex}",
                "action": {
                  "label": "action",
                  "type": "uri",
                  "uri": "${data.website}"
                },
                "contents": [
                  {
                    "aspectMode": "fit",
                    "size": "md",
                    "align": "center",
                    "type": "image",
                    "url": "${data.logo}"
                  },
                  {
                    "type": "text",
                    "text": "${data.company}",
                    "color": "${data.companyColor}",
                    "weight": "bold",
                    "size": "xs",
                    "align": "center"
                  },
                  {
                    "type": "text",
                    "text": "${data.companyEN}",
                    "color": "${data.companyColor}",
                    "size": "xxs",
                    "align": "center"
                  }
                ]
              },
              {
                "flex": "${data.middleFlex}",
                "layout": "vertical",
                "margin": "30px",
                "spacing": "sm",
                "type": "box",
                "alignItems": "center",
                "contents": [
                  {
                    "size": "md",
                    "text": "${data.title} ${data.titleEN}",
                    "type": "text",
                    "color": "${data.textColor}",
                    "wrap": true
                  },
                  {
                    "type": "text",
                    "text": "${data.name} ${data.nameEN}",
                    "color": "${data.nameColor}",
                    "size": "xl",
                    "weight": "bold",
                    "margin": "none"
                  }
                ]
              },
              {
                "layout": "vertical",
                "type": "box",
                "flex": "${data.bottomFlex}",
                "contents": [
                  {
                    "layout": "horizontal",
                    "margin": "xl",
                    "spacing": "md",
                    "type": "box",
                    "contents": [
                      {
                        "flex": 0,
                        "layout": "vertical",
                        "type": "box",
                        "contents": [
                          {
                            "align": "center",
                            "flex": 0,
                            "size": "md",
                            "text": "‚úâ",
                            "type": "text",
                            "color": "${data.textColor}",
                            "weight": "bold"
                          },
                          {
                            "align": "center",
                            "flex": 0,
                            "size": "md",
                            "text": "üìû",
                            "type": "text",
                            "color": "${data.textColor}",
                            "weight": "bold"
                          },
                          {
                            "align": "center",
                            "flex": 0,
                            "size": "md",
                            "text": "üåê",
                            "type": "text",
                            "color": "${data.textColor}",
                            "weight": "bold"
                          },
                          {
                            "align": "center",
                            "flex": 0,
                            "size": "md",
                            "text": "üè†",
                            "type": "text",
                            "color": "${data.textColor}",
                            "weight": "bold"
                          }
                        ]
                      },
                      {
                        "layout": "vertical",
                        "type": "box",
                        "contents": [
                          {
                            "size": "md",
                            "text": "${data.email}",
                            "type": "text",
                            "color": "${data.textColor}",
                            "maxLines": 1,
                            "action": {
                              "label": "action",
                              "type": "uri",
                              "uri": "mailto:${data.email}"
                            }
                          },
                          {
                            "size": "md",
                            "text": "${data.phone}",
                            "type": "text",
                            "color": "${data.textColor}",
                            "maxLines": 1,
                            "action": {
                              "label": "action",
                              "type": "uri",
                              "uri": "tel:${data.phone}"
                            }
                          },
                          {
                            "size": "md",
                            "text": "${data.website}",
                            "type": "text",
                            "color": "${data.textColor}",
                            "maxLines": 1,
                            "action": {
                              "label": "action",
                              "type": "uri",
                              "uri": "${data.website}"
                            }
                          },
                          {
                            "size": "md",
                            "text": "${data.address}",
                            "type": "text",
                            "color": "${data.textColor}",
                            "lineSpacing": "12px",
                            "action": {
                              "label": "action",
                              "type": "uri",
                              "uri": "https://www.google.com/maps/search/${data.address}"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  }
]
